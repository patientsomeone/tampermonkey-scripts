{
  "compileOnSave": false,
  "rules": {
    "class-name": true
  },
  "compilerOptions": {
    "resolveJsonModule": true,
    "target": "es6",
    "module": "commonjs",
    "outDir": ".dist",
    "sourceMap": false
  },
  "include": [".src/**/*.ts", "*.ts", "**/.*.ts", ".src/.i.*"],
  "exclude": ["**/node_modules/**", "**/*.spec.ts", "**/*.ts.i.*"],
  "types": ["google-apps-script"]
}
