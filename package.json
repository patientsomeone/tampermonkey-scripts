    "license": "ISC",
    "bugs": {
        "url": "https://gitlab.com/robinsolutions/tampermonkey-scripts/issues"
    },
    "homepage": "https://gitlab.com/robinsolutions/tampermonkey-scripts#README",
    "dependencies": {
        "async": "^2.5.0",
        "atob": "^2.1.2",
        "btoa": "^1.2.1",
        "csv-parse": "^1.2.3",
        "debug": "^3.1.0",
        "excel4node": "^1.7.2",
        "fast-csv": "^2.4.1",
        "jsdom": "^15.1.1",
        "JSONStream": "^1.3.5",
        "performance": "^1.1.1",
        "performance-now": "^2.1.0",
        "unirest": "^0.6.0"
    },
