{
    "name": "tampermonkey-scripts",
    "version": "1.0.0",
    "description": "Various Tamper Monkey scripts for use",
    "main": "index.js",
    "scripts": {
        "push": "echo 'Building' && (tsc || echo 'TSC build failed' --success = first) && echo 'Pushing to Google Apps' && (clasp push || echo 'Clasp Push Failed') && echo 'Complete'",
        "lint": "eslint --ext .ts ./.src",
        "build": "tsc",
        "newBranch": "gulp createBranch",
        "pullRequest": "gulp createPullRequest",
        "update": "gulp pullMaster",
        "prune": "gulp prune"
    },
    "repository": {
        "type": "git",
        "url": "git@gitlab.com:robinsolutions/tampermonkey-scripts.git"
    },
    "keywords": [
        "CSV",
        "Regex"
    ],
